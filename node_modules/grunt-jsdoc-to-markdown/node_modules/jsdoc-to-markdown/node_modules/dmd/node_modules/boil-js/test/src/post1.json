{
    "post": {
        "template": "post.hbs",
        "data": {
            "article": {
                "filename": "test/src/optionsInSrc.hbs",
                "title": "{{front-matter (read 'test/src/optionsInSrc.hbs') 'one'}}",
                "author": "{{front-matter (read 'test/src/optionsInSrc.hbs') 'two'}}",

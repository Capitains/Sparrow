{
  "name": "karma-jasmine-jquery",
  "version": "0.1.0",
  "homepage": "https://github.com/bessdsv/karma-jasmine-jquery",
  "authors": [
    "Sergey Dolinkov"
  ],
  "description": "Jasmine-jquery plugin for Jasmine in karma",
  "license": "BSD",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.1",
    "jasmine-jquery": "~2.0.5"
  },
  "keywords": [
    "karma",
    "jasmine",
    "jquery",
    "karma-plugin",
    "karma-jasmine",
    "jasmine-jquery"
  ],
  "install": {
    "path": {
      "jquery" : "lib",
      "jasmine-jquery": "lib",
      "jasmine": "lib"
    },
    "sources": {
      "jasmine-jquery": [
         "bower_components/jasmine-jquery/lib/jasmine-jquery.js"
      ],
      "jquery": {
        "mapping": [
          {"bower_components/jquery/dist/jquery.js": "jqueryForJasmineJqueryPlugin.js"}
        ]
      }
    },
    "scripts" : {
      "postinstall" : {
        "jasmine-jquery" : "scripts/JasmineJqueryPluginPostInstallScript.js"
      }
    }
  }
}
